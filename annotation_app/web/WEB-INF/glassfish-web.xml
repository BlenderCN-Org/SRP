<?xml version="1.0" encoding="UTF-8"?>
<glassfish-web-app error-url="">
    <context-root></context-root>
    <servlet>
        <servlet-name>SceneAnnotator</servlet-name>
        <servlet-class>source.SceneAnnotator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SceneAnnotator</servlet-name>
        <url-pattern>/SceneAnnotator</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>jsp/index.jsp</welcome-file>
    </welcome-file-list>
</glassfish-web-app>
